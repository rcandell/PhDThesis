@proceeding{4628,
	title = {Model-Based Integration Platform for FMI Co-Simulation and Heterogeneous Simulations of Cyber-Physical Systems},
     author = {	H. Neema and J. Gohl and Z. Lattmann and J. Sztipanovits and G. Karsai and S. Neema and T. Bapty and J. Batteh and H. Tummescheit and C. Sureshkumar},
     author ={},
