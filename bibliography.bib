@inproceedings{10.1007/978-3-319-07788-8_40,
	title        = {Denial-of-Service Attacks in Wireless Networks Using Off-the-Shelf Hardware},
	author       = {Fragkiadakis, Alexandros and Askoxylakis, Ioannis and Chatziadam, Panos},
