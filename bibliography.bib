@inproceedings{CandellISIE2019.Conf,
	title        = {Wireless Interference Estimation Using Machine Learning in a Robotic Force-Seeking Scenario},
	author       = {R. {Candell} and K. {Montgomery} and M. {Kashef} and Y. {Liu} and S. {Foufou}},
	year         = 2019,
	month        = jun,
	booktitle    = {2019 IEEE 28th International Symposium on Industrial Electronics (ISIE)},
	pages        = {1334--1341},
	doi          = {10.1109/ISIE.2019.8781418},
	issn         = {2163-5137},
	abstract     = {Cyber-physical systems are systems governed by the laws of physics that are tightly controlled by computer-based algorithms and network-based sensing and actuation. Wireless communication technology is envisioned to play a primary role in conducting the information flows within such systems. A practical industrial wireless use case involving a robot manipulator control system, an integrated wireless force-torque sensor, and a remote vision-based observer is constructed and the performance of the cyber-physical system is examined. By using readings from the remote observer, an estimation system is developed using machine learning regression techniques. We demonstrate the practicality of combining statistical analysis with machine learning to indirectly estimate signal-to-interference of the wireless communication link using measurements from the remote observer. Results from the statistical analysis and the performance of the machine learning system are presented.},
	keywords     = {control engineering computing;cyber-physical systems;estimation theory;force measurement;force sensors;learning (artificial intelligence);manipulators;observers;radio links;radiofrequency interference;regression analysis;statistical analysis;telecommunication computing;telecommunication control;torque measurement;wireless sensor networks;robotic force-seeking scenario;cyber-physical system;computer-based algorithms;network-based sensing;wireless communication technology;robot manipulator control system;integrated wireless force-torque sensor;remote vision-based observer;wireless communication link;wireless interference estimation system;network-based actuation;machine learning regression techniques;statistical analysis;signal-to-interference estimation;Wireless communication;Interference;Wireless sensor networks;Robot sensing systems;Manipulators;Force;industrial wireless;802.11;factory communications;cyber-physical systems;wireless networking;robotics}
}
@article{CandellIJAMT2020.Jrnl,
	title        = {Machine Learning Based Wireless Interference Estimation in a Robotic Force-Seeking Application},
	author       = {R. {Candell} and K. {Montgomery} and M. {Kashef} and Y. {Liu} and S. {Foufou}},
	year         = 2020,
	journal      = {Engineering Applications of Artificial Intelligence},
	doi          = {pending},
	journaltitle = {Engineering Applications of Artificial Intelligence},
	abstract     = {Wireless communications plays an essential role in the future cyber-physical systems vision which includes having more sensors and actuators, and, hence, more information transferred through wireless. In this article, we consider an industrial use case of a robot arm control system equipped with a force-torque sensor. Movement of the arm is tracked by a vision-based ground truth measurement system. Movement of the arm is controlled by a robot controller applying a downward pressure on a spring assembly until a predetermined force is detected. The remote vision-based observer provides readings about the position of the robot arm where these readings are used to estimate the signal-to-interference ratio of the wireless link. A supervised machine learning approach is used for the wireless channel quality estimation. In this paper, we study the impact of various system features on the performance of various machine learning algorithms and compare their performance. Moreover, we investigate the impact of the training and the estimation period on the performance of the proposed approach. The results provide insights about the impact of wireless communications on cyber-physical systems and an example of employing machine learning to improve industrial wireless deployments.},
	keywords     = {control engineering computing;cyber-physical systems;estimation theory;force measurement;force sensors;learning (artificial intelligence);manipulators;observers;radio links;radiofrequency interference;regression analysis;statistical analysis;telecommunication computing;telecommunication control;torque measurement;wireless sensor networks;robotic force-seeking scenario;cyber-physical system;computer-based algorithms;network-based sensing;wireless communication technology;robot manipulator control system;integrated wireless force-torque sensor;remote vision-based observer;wireless communication link;wireless interference estimation system;network-based actuation;machine learning regression techniques;statistical analysis;signal-to-interference estimation;Wireless communication;Interference;Wireless sensor networks;Robot sensing systems;Manipulators;Force;industrial wireless;802.11;factory communications;cyber-physical systems;wireless networking;robotics}
}
@article{Martinez2019,
	title        = {A Square Peg in a Round Hole: The Complex Path for Wireless in the Manufacturing Industry},
	author       = {B. {Martinez} and C. {Cano} and X. {Vilajosana}},
	year         = 2019,
	month        = apr,
	journal      = {IEEE Communications Magazine},
	volume       = 57,
	number       = 4,
	pages        = {109--115},
	doi          = {10.1109/MCOM.2019.1800570},
	issn         = {1558-1896},
	abstract     = {The manufacturing industry is at the edge of the fourth industrial revolution, a paradigm of integrated architectures in which the entire production chain (composed of machines, workers, and products) is intrinsically connected. Wireless technologies can add further value in this manufacturing revolution. However, we identify some signs which indicate that wireless technology could be left out of the next generation of smart factory equipment. This is particularly relevant considering that the heavy machinery characteristic of this sector can last for decades. We argue that at the core of this issue there is a mismatch between industrial needs and the interests of academic and partly academic sectors (e.g., standardization bodies). We base our claims on surveys from renowned advisory firms and interviews with industrial actors, which we contrast with results from content analysis of scientific articles. Finally, we propose some convergence paths that, while still retaining the degree of novelty required for academic purposes, are more aligned with industrial concerns.},
	keywords     = {innovation management;manufacturing industries;production engineering computing;production equipment;production facilities;integrated architectures;wireless technology;manufacturing revolution;smart factory equipment;heavy machinery characteristic;industrial actors;convergence paths;complex path;manufacturing industry;fourth industrial revolution;production chain;academic sectors;content analysis;Wireless communication;Wireless sensor networks;Manufacturing industries;Communication system security;Protocols;Manufacturing}
}
